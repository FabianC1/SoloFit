{
  "expo": {
    "name": "SoloFit",
    "slug": "SoloFit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AppIcon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/AppIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },

    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/AppIcon.png",
        "resizeMode": "contain",
        "backgroundColor": "#111827"
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AppIcon.png",
        "backgroundColor": "#111827"
      },
      "splash": {
        "image": "./assets/AppIcon.png",
        "resizeMode": "contain",
        "backgroundColor": "#111827"
      },
      "edgeToEdgeEnabled": true
    },

    "web": {
      "favicon": "./assets/AppIcon.png"
    }
  }
}
